-UdevEvent *udev_event_new(sd_device *dev, usec_t exec_delay_usec, sd_netlink *rtnl) {
-        UdevEvent *event;
-
-        assert(dev);
-
-        event = new(UdevEvent, 1);
-        if (!event)
-                return NULL;
