-        /* Now get the full feature buffer */
-        scsi_cmd_init(&sc);
-        scsi_cmd_set(&sc, 0, 0x46);
-        scsi_cmd_set(&sc, 7, ( len >> 8 ) & 0xff);
-        scsi_cmd_set(&sc, 8, len & 0xff);
-        scsi_cmd_set(&sc, 9, 0);
