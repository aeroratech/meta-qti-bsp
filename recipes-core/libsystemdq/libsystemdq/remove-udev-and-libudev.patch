-static UdevRuleOperatorType parse_operator(const char *op) {
-        assert(op);
-
-        if (startswith(op, "=="))
-                return OP_MATCH;
-        if (startswith(op, "!="))
-                return OP_NOMATCH;
-        if (startswith(op, "+="))
-                return OP_ADD;
-        if (startswith(op, "-="))
-                return OP_REMOVE;
-        if (startswith(op, "="))
-                return OP_ASSIGN;
-        if (startswith(op, ":="))
-                return OP_ASSIGN_FINAL;
-
-        return _OP_TYPE_INVALID;
-}
