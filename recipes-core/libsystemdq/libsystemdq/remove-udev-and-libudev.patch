-        cur_profile = features[6] << 8 | features[7];
-        if (cur_profile > 0) {
-                log_debug("current profile 0x%02x", cur_profile);
-                feature_profile_media(cur_profile);
-                ret = 0; /* we have media */
-        } else
-                log_debug("no current profile, assuming no media");
