-        r = getpeercred(sock, &ucred);
-        if (r < 0) {
-                log_error_errno(r, "Failed to receive credentials of ctrl connection: %m");
-                return 0;
-        }
-
-        if (ucred.uid > 0) {
-                log_error("Invalid sender uid "UID_FMT", closing connection", ucred.uid);
-                return 0;
