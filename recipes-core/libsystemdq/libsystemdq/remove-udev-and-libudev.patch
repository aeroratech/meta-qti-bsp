-
-                subst_type = rule_get_substitution_type((const char*) data);
-        }
-
-        token = new(UdevRuleToken, 1);
-        if (!token)
-                return -ENOMEM;
-
-        *token = (UdevRuleToken) {
-                .type = type,
-                .op = op,
-                .value = value,
-                .data = data,
-                .match_type = match_type,
-                .attr_subst_type = subst_type,
-                .attr_match_remove_trailing_whitespace = remove_trailing_whitespace,
