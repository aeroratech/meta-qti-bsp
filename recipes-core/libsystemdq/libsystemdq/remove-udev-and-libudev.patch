-#define HID_RPTDESC_FIRST_BYTE_LONG_ITEM 0xfeu
-#define HID_RPTDESC_TYPE_GLOBAL 0x1u
-#define HID_RPTDESC_TYPE_LOCAL 0x2u
-#define HID_RPTDESC_TAG_USAGE_PAGE 0x0u
-#define HID_RPTDESC_TAG_USAGE 0x0u
