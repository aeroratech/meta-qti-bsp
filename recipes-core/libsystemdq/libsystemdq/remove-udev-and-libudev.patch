-                                 */
-                                return log_debug_errno(SYNTHETIC_ERRNO(EINVAL),
-                                                       "%s: sense result too small %d bytes",
-                                                       dev_scsi->kernel, s);
-
-                        asc = sense_buffer[12];
-                        ascq = sense_buffer[13];
-                } else if (IN_SET(code, 0x2, 0x3)) {
-                        sense_key = sense_buffer[1] & 0xf;
