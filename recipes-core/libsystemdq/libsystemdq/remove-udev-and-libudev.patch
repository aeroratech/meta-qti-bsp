-
-        else if (IN_SET(token->type, TK_A_DEVLINK,
-                        TK_A_OWNER, TK_A_GROUP, TK_A_MODE,
-                        TK_A_OWNER_ID, TK_A_GROUP_ID, TK_A_MODE_ID))
-                SET_FLAG(rule_line->type, LINE_HAS_DEVLINK, true);
-
-        else if (token->type == TK_A_OPTIONS_STATIC_NODE)
